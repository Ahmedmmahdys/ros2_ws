<?xml version="1.0" ?>
<robot name="towercrane" xmlns:xacro="http://ros.org/wiki/xacro">
  

  <!-- Include materials -->
  <xacro:include filename="$(find multi_robot_sim)/urdf/materials.xacro"/>

  <!-- Include trolly depth_camera-->
  <xacro:include filename="$(find multi_robot_sim)/urdf/tc_depth_cam.xacro"/>

  <!-- Include trolly depth_camera-->
  <!-- <xacro:include filename="$(find multi_robot_sim)/urdf/ur_depth_cam.xacro"/> -->

  <!-- using a macro to calculate the inertia-->
  <xacro:include filename="$(find multi_robot_sim)/urdf/inertial_calculate.xacro" />


  <!-- Main agrs -->
  <xacro:arg name="tf_prefix" default=""/>
  <xacro:property name="tf_prefix" value="$(arg tf_prefix)"/>
  <xacro:arg name="robot_name" default="robot_2"/>
  <xacro:property name="robot_name" value="$(arg robot_name)"/>

  <!-- Useful XACRO Variables (Properties) -->
    <xacro:property name="PI_5" value="15.707963268" />
    <xacro:property name="PI" value="3.14159265359" />
    <xacro:property name="PI_2" value="1.570796327" />
    <xacro:property name="PI_4" value="0.785398163" />
    <xacro:property name="PI_18" value="0.174532925" />
    <xacro:property name="D_160" value="2.792526803" />
    <xacro:property name="D_200" value="3.490658504" />
    <xacro:property name="effort" value="30000000000.0" />
    <xacro:property name="velocity" value="30000.0" />

    <!-- Useful Macros -->
    <xacro:macro name="default_inertial" params="mass">
        <inertial>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <mass value="${mass}" />
            <inertia ixx="0.5" ixy="0.0" ixz="0.0"
                     iyy="0.5" iyz="0.0"
                     izz="0.5" />
        </inertial>
    </xacro:macro>

  <!-- World link -->
  <link name="world"/>

